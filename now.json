{
  "version": 2,
  "name": "alehatsman.com",
  "alias": "alehatsman.com",
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "headers": [
    {
      "source": "/static/fonts/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000"
        }
      ]
    }
  ]
}
